{
  "name": "ar-body-measurements",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "run:ios": "expo run:ios",
    "run:android": "expo run:android",
    "validate-assets": "node validate-assets.js",
    "verify-build": "node verify-build.js",
    "prebuild:ios": "npm run verify-build && expo prebuild --platform ios",
    "prebuild:android": "npm run verify-build && expo prebuild --platform android",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:production": "eas build --platform android --profile production",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:ios:production": "eas build --platform ios --profile production",
    "build:all:preview": "eas build --platform all --profile preview",
    "build:all:production": "eas build --platform all --profile production",
    "build:ios:enhanced": "scripts/enhanced-build.sh production ios",
    "build:ios:enhanced:debug": "scripts/ios-build.sh production true true true true",
    "build:ios:enhanced:release": "scripts/ios-build.sh production true false true true",
    "build:ios:enhanced:metal": "scripts/ios-build.sh production true false true true",
    "build:ios:enhanced:coreml": "scripts/ios-build.sh production true false true true",
    "build:ios:enhanced:win": "scripts/ios-build.bat production true false true true",
    "build:ios:enhanced:win:debug": "scripts/ios-build.bat production true true true true",
    "build:ios:enhanced:win:release": "scripts/ios-build.bat production true false true true",
    "ios:arkit6": "scripts/ios-arkit6-setup.sh",
    "ios:performance": "scripts/ios-performance-monitor.sh",
    "ios:accuracy": "scripts/ios-accuracy-enhancement.sh",
    "ios:optimize": "scripts/ios-optimization.sh",
    "ios:test": "scripts/ios-test.sh",
    "ios:deploy": "scripts/ios-deploy.sh",
    "build:clean": "rm -rf node_modules/.cache .expo android/build ios/build && npm install",
    "build:enhanced": "scripts/enhanced-build.sh",
    "build:enhanced:win": "scripts/enhanced-build.bat",
    "build:android:enhanced": "scripts/enhanced-build.sh production android",
    "build:all:enhanced": "scripts/enhanced-build.sh production all",
    "deploy:android": "eas build --platform android --profile production --non-interactive",
    "deploy:ios": "eas build --platform ios --profile production --non-interactive",
    "deploy:all": "npm run deploy:android && npm run deploy:ios",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "submit:all": "eas submit --platform all",
    "update:android": "eas update --platform android",
    "update:ios": "eas update --platform ios",
    "update:all": "npm run update:android && npm run update:ios",
    "doctor": "eas doctor",
    "whoami": "eas whoami",
    "login": "eas login",
    "logout": "eas logout",
    "config": "eas config",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:clean:ios": "expo prebuild --platform ios --clean",
    "prebuild:clean:android": "expo prebuild --platform android --clean",
    "prebuild:clean:all": "npm run prebuild:clean:ios && npm run prebuild:clean:android",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf node_modules .expo android/build ios/build",
    "clean:win": "rmdir /s /q node_modules .expo android\\build ios\\build",
    "reset": "npm run clean && npm install",
    "reset:win": "npm run clean:win && npm install",
    "postinstall": "patch-package && node scripts/fix-gradle-comprehensive.js",
    "test:gradle": "node scripts/test-gradle-fix.js",
    "fix:gradle": "node scripts/quick-gradle-fix.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.0.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "expo": "~49.0.0",
    "expo-asset": "~8.0.0",
    "expo-camera": "~13.0.0",
    "expo-constants": "~14.0.0",
    "expo-dev-client": "~2.0.0",
    "expo-font": "~11.0.0",
    "expo-gl": "~12.0.0",
    "expo-media-library": "~14.0.0",
    "expo-modules-autolinking": "~1.0.0",
    "expo-modules-core": "~1.0.0",
    "expo-sensors": "~11.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-vision-camera": "^4.7.2",
    "react-native-web": "~0.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^10.0.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-native": "^4.1.0",
    "patch-package": "^8.0.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
